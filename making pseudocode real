FUNCTION LEFT (string: STRING, length: INTEGER) RETURNS STRING
  RETURUN string [0:length]
ENDFUNCTION

FUNCTION RIGHT (string: STRING, length: INTEGER) RETURNS STRING
  RETURUN string [0:length]
ENDFUNCTION

FUNCTION MID (string: STRING, start: INTEGER, end: INTEGER) RETURNS STRING
  RETURUN string [start:end]
ENDFUNCTION

FUNCTION LENGTH (string: STRING) RETURNS INTEGER
  RETURN LENGTH (string)
ENDFUNCTION

FUNCTION LCASE (char: CHAR) RETURNS CHAR
  RETURUN char
ENDFUNCTION

FUNCTION UCASE (char: CHAR) RETURNS CHAR
  RETURUN char
ENDFUNCTION

FUNCTION TO_UPPER (string: STRING) RETURNS STRING
  RETURUN TO_UPPER (string)
ENDFUNCTION

FUNCTION TO_LOWER (string: STRING) RETURNS STRING
  RETURUN TO_LOWER (string)
ENDFUNCTION

FUNCTION NUM_TO_STRING (num: REAL) RETURNS STRING
  string = "f{num}"
  RETURN string
ENDFUNCTION

